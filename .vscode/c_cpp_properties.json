{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include/SDL2"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "intelliSenseMode": "gcc-x64",
            "compilerArgs": [
                "-Wall",
                "-Wextra",
                "-Wpedantic",
                "-Werror",
                "-Wformat=2",
                "-Wno-unused-parameter",
                "-Wshadow",
                "-Wwrite-strings",
                "-Wstrict-prototypes",
                "-Wold-style-definition",
                "-Wredundant-decls",
                "-Wnested-externs",
                "-Wmissing-include-dirs"
            ],
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}